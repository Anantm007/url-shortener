<html>
  <head>
    <link href="/css/styles.css" rel="stylesheet" type="text/css" />
    <!-- Latest compiled and minified CSS -->
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
    />

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <!--Navigation bar-->
    <% include ./includes/navbar.ejs %>
  </head>
  <div class="container">
    <h2 style="color: black; margin-top: 3rem">
      Find all the created URLs (only the latest 200)
    </h2>
    <br />
    <% urls.forEach(function(url) { %>

    <div class="card" style="background: #512c96">
      <div class="card-body">
        <h5 class="card-title">
          <a href="/<%=url.code%>" style="color: #f9c6ba"><%=url.shorturl%></a>
        </h5>
        <p class="card-text">Original URL: <%=url.longurl%></p>
        <p class="card-text">Date Created: <%=url.date%></p>
        <p class="card-text">clicks: <%=url.clicks%></p>
      </div>
    </div>
    <br />
    <% }); %>
  </div>
  <body></body>
</html>
